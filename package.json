        "monaco",
        "intelliSense"
    ],
    "author": "Sergi Philipsen <philipsen.sergi@gmail.com> (https://philipsens.nl)",
    "maintainers": [
        "philipsens"
    ],
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-env": "^7.11.5",
        "babel-loader": "^8.1.0",
        "husky": "^4.3.0",
        "monaco-editor-core": "^0.21.2",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.0.2",
        "raw-loader": "^4.0.1",
        "webpack": "^4.44.1",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "turndown": "^6.0.0",

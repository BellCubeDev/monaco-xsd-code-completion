{
    "compilerOptions": {
        "target": "es5",
        "lib": ["webworker", "es2019"],
        "typeRoots": ["./node_modules/@types/", "./src/@types"],
        "noImplicitAny": false,
        "sourceMap": true,
        "downlevelIteration": true,
        "strict": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "rootDir": "src"
    }
}
